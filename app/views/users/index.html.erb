<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <div class="well users">
        <h2> All Users </h2> <br/>
        <p>
        <% @users.each do |user| %>
          <% if user.id == session[:user_id] %>
            <%= link_to user.user_name, profile_path(user) %> <br/>
          <% else %>
            <%= link_to user.user_name, user_path(user) %> <br/>
          <% end %>
        <% end %>
        </p>
      </div>
    </div>
  </div>
</div>
