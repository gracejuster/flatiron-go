<%= form_for @meetup do |f| %>
  <%= f.label :set_date %>
  <%= f.collection_select :set_date, @days, :to_date, :to_s %><br>
  <%= f.label :set_hour_min %>
  <%= f.collection_select :set_hour_min, @times, :to_s, :to_s %><br>
  <%= f.label :set_location %>
  <%= f.collection_select :location_id, Location.all, :id, :name %><br>
  <%= f.label :title%> 
  <%= f.text_field :title %><br>
  <%= f.label :description %><br>
  <%= f.text_area :description %><br>
  <%= f.submit "Create Meetup" %> 
<%end%>
